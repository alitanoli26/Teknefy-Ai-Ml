   "source": [
    "#Matrix Operatons include Dot Product and transpose \n",
    "# For dot product \n",
    "import numpy as np\n",
    "A= np.array([[1, 2], [3, 4]])\n",
    "B= np.array([[5, 6], [7, 8]])\n",
    "\n",
    "print(\"Dot product of A and B is \\n\", np.dot(A, B))\n",
    "\n"
   ]
