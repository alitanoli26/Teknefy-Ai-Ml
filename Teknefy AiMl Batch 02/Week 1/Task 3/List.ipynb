    "# A list stores multiple items and we can CHANGE these items.\n",
    "\n",
    "fruits = [\"apple\", \"banana\", \"mango\"]\n",
    "\n",
    "\n",
    "fruits.append(\"orange\")  # Adds a new item at the end of the list.\n",
    "\n",
    "\n",
    "fruits.remove(\"banana\") # Removes the first matching item from the list.\n",
    "\n",
    "print(fruits)\n"
   ]
