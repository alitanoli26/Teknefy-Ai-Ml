   "source": [
    "class Animal:\n",
    "    def sound(self):\n",
    "        print(\"Animal sound\")\n",
    "\n",
    "class Cat(Animal):\n",
    "    # Child class CHANGES the parent’s method.\n",
    "    # Same name → different behavior → polymorphism\n",
    "    def sound(self):\n",
    "        print(\"Meow\")\n",
    "\n",
    "c = Cat()\n",
    "c.sound()  # overridden version runs\n"
   ]
  }
 ],
